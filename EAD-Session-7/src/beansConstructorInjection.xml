<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
    <bean id="textEditor" class="com.metacube.training.dependencyInjection.constructorInjection.TextEditor" >
        <constructor-arg ref="spellChecker" ></constructor-arg>
    </bean>
    <bean id="spellChecker" class="com.metacube.training.dependencyInjection.constructorInjection.SpellChecker">
        <constructor-arg value="checking spelling" >
        </constructor-arg>
    </bean>
</beans>