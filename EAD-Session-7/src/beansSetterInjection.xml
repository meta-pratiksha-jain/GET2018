<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
    <bean id="textEditor" class="com.metacube.training.dependencyInjection.setterInjection.TextEditor" >
        <property name="spellChecking" ref="spellChecker"></property>
    </bean>
    <bean id="spellChecker" class="com.metacube.training.dependencyInjection.setterInjection.SpellChecker">
        <property name="spellChecking" value="checking spelling" >
        </property>
    </bean>
</beans>